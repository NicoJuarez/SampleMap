<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/functionalities_graph"
    app:startDestination="@id/functionalitiesFragment">

    <fragment
        android:id="@+id/functionalitiesFragment"
        android:name="com.alansnaki.samplemap.ui.fragments.FunctionalitiesFragment"
        android:label="fragment_functionalities"
        tools:layout="@layout/fragment_functionalities" >
        <action
            android:id="@+id/action_functionalitiesFragment_to_mapFragment"
            app:destination="@id/mapFragment" />
    </fragment>
    <fragment
        android:id="@+id/mapFragment"
        android:name="com.alansnaki.samplemap.ui.fragments.MapFragment"
        android:label="fragment_map"
        tools:layout="@layout/fragment_map" />
</navigation>